<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Persistences.Scheduling" table="SCHEDULING">
    <id column="ID" name="id" type="int">
      <generator class="identity"/>
    </id>
    <property column="TIME" name="time" type="int"/>
    <property column="MINUTE" name="minute" type="int"/>
    <property column="DATESCHEDULE" name="dateschedule" type="date"/>
  </class>
</hibernate-mapping>
